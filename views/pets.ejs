<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
</head>

<body>
    <h1>
        <%= title %>
    </h1>
    <ul>
        <% for (const pet of petType) { %>
            <li><a href="/pet/<%= pet.breed.toLowerCase() %>/<%= petType.indexOf(pet) %>">
                    <%= pet.name %>
                </a></li>
            <% } %>
    </ul>
    <a href="/">Go back to homepage</a>
</body>

</html>